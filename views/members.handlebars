<div class="container containerMembers container-fluid">
	<div class="row head1 text">
		<div class="col-md-12 text">
			<p class="text2">Learn web development in a collaborative setting</p>
			<hr />
		</div>
	</div>
	<div class="row head1">
		<div class="col-md-4">
			<h1 class="text"><a href="#">CODEBASICS.COM<br /><span class="h1-small">Online Portal</span></a></h1>
		</div>
		<div class="col-md-8"></div>
	</div>
	<div class="row">
		<div class="col-md-2"></div>
		<div class="col-md-8 text-center">
			<div>
				<h2 class="welcome mt-4">Welcome <span class="member-name">techmedia.lg@gmail.com</span> </h2>
			</div>
			<div class="student-dash-cont">
				<h2 class="student-dash-head">STUDENT DASHBOARD</h2>
				<a class="navbar-brand" href="/logout">Logout</a>
			</div>
		</div>
		<div class="col-md-2"></div>
	</div>
	<br>
	<br>
	<div class="row">


		<div class="row task-container justify-content-center">
			<div class="col-md-3"></div>
			<div class="col-md-6">
				<div class="course-head">
					<h3 class="text-center"><strong>Front-end Web development technologies</strong></h3>
					<p class="text-center">Please follow the resource link to find and add more topics.</p>
				</div>
			</div>
			<div class="col-md-3"></div>
		</div>
		<div class="row task-container justify-content-center">
			<div class="col-md-2"></div>
			<div class="col-md-5">
				<h3><strong>HTML:</strong></h3>
				<h5><a href="https://html.com/">Resources</a></h5>
				<ol id="unfinishedHtml">
					{{#each Html}}
					{{#unless this.dataValues.html_condition}}
					<li>
						<p>
							<button type='submit' id={{this.dataValues.id}} class="complete-html btn btn-sm btn-primary"
								data-id="{{this.dataValues.id}}"><i class="far fa-check-square"></i></button>
							{{this.dataValues.html_name}}

						</p>
					</li>
					{{/unless}}
					{{/each}}
				</ol>
				<form class="create-update-form" action="api/insertHtml" method="POST">
					<div>
						<textarea type="text" placeholder="Type..." name="name"></textarea><br>
						<button class="btn btn-primary" type="submit">Add topic</button>
					</div>
				</form>
			</div>
			<div class="col-md-5">
				<h3><strong>CSS:</strong></h3>
				<h5><a href="https://devdocs.io/css/">Resources</a></h5>
				<ol id="unfinishedCss">
					{{#each Css}}
					{{#unless this.dataValues.css_condition}}
					<li>
						<p>
							<button type='submit' id={{this.dataValues.id}} class="complete-css btn btn-sm btn-primary"
								data-id="{{this.dataValues.id}}"><i class="far fa-check-square"></i></button>
							{{this.dataValues.css_name}}

						</p>
					</li>
					{{/unless}}
					{{/each}}
				</ol>
				<form class="create-update-form" action="api/insertCss" method="POST">
					<div>
						<textarea type="text" placeholder="Type..." name="name"></textarea><br>
						<button class="btn btn-primary" type="submit">Add topic</button>
					</div>
				</form>
			</div>
		</div>
